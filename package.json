{
  "dependencies": {
    "@sendgrid/mail": "^7.4.7",
    "aws-sdk": "^2.999.0",
    "bcrypt": "5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "otplib": "^12.0.1",
    "qrcode": "^1.4.4",
    "sharp": "^0.29.1",
    "socket.io": "^4.2.0",
    "socketio-jwt": "^4.6.2",
    "uuid": "^8.3.2",
    "validator": "^13.6.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon --inspect src/index.js -e js",
    "format": "prettier --single-quote --trailing-comma all --tab-width 4 --write src/**/*.{js,json,scss}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix src/",
      "tslint --fix --project .",
      "git add"
    ]
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1"
  }
}
